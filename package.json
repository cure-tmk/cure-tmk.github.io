{
  "name": "cure-tmk.github.io",
  "version": "1.0.0",
  "license": "ISC",
  "author": "@cure-tmk",
  "scripts": {
    "build": "astro build",
    "dev": "astro dev",
    "test": "vitest",
    "typecheck": "run-p typecheck:*",
    "typecheck:script": "tsc -p .",
    "typecheck:astro": "astro check",
    "format": "run-s 'format:*'",
    "format:prettier": "prettier --config prettier.config.js --write '**/*.+(ts|tsx|js|md|json|astro)'",
    "format:script": "eslint --fix --ext .js,.ts,.tsx,.astro .",
    "lint": "run-s 'lint:*'",
    "lint:script": "eslint --ext .js,.ts,.astro .",
    "preview": "astro preview",
    "start": "astro preview",
    "scaffold": "scaffdog generate -f"
  },
  "dependencies": {
    "@astrojs/image": "0.17.3",
    "@astrojs/tailwind": "4.0.0",
    "astro": "2.10.5",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-astro": "0.28.0",
    "eslint-plugin-import": "2.28.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "0.11.0",
    "prettier-plugin-packagejson": "2.4.5",
    "prettier-plugin-scaffdog": "2.5.1",
    "prettier-plugin-tailwindcss": "0.5.2",
    "scaffdog": "2.5.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vitest": "0.34.1"
  },
  "packageManager": "pnpm@8.6.12",
  "engines": {
    "node": "18.17.1"
  }
}
